{
  "name": "@odmd-rag/document-ingestion",
  "version": "1.0.0",
  "description": "RAG Document Ingestion Service - OndemandEnv Platform",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "rag-document-ingestion": "bin/rag-document-ingestion.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "clean": "rimraf dist/",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "cdk": "cdk",
    "cdk-ls": "tsc --build && cross-env ODMD_build_id=ragDocumentIngestion ODMD_rev_ref=b..dev cdk ls --show-dependencies --profile odmd-rag-ws1",
    "cdk-synth": "tsc --build && cross-env ODMD_build_id=ragDocumentIngestion ODMD_rev_ref=b..dev cdk synth --all --profile odmd-rag-ws1",
    "cdk-deploy": "tsc --build && cross-env ODMD_build_id=ragDocumentIngestion ODMD_rev_ref=b..dev cdk deploy --all",
    "package": "npm run build && npm pack",
    "start": "node dist/app.js"
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity": "^3.826.0",
    "@aws-sdk/client-eventbridge": "^3.826.0",
    "@aws-sdk/client-s3": "^3.826.0",
    "@aws-sdk/s3-presigned-post": "^3.826.0",
    "@aws-sdk/s3-request-presigner": "^3.826.0",
    "aws-cdk-lib": "2.200.1",
    "constructs": "^10.0.0",
    "mime-types": "^2.1.35",
    "source-map-support": "^0.5.21",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@odmd-rag/contracts-lib-rag": "1.0.7",
    "@types/aws-lambda": "^8.10.119",
    "@types/jest": "^29.5.14",
    "@types/mime-types": "^2.1.1",
    "@types/node": "^20.5.0",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.47.0",
    "jest": "^29.7.0",
    "rimraf": "^5.0.0",
    "ts-jest": "^29.3.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.1.6"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src",
      "<rootDir>/tests"
    ],
    "testMatch": [
      "**/__tests__/**/*.ts",
      "**/?(*.)+(spec|test).ts"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.d.ts",
      "!src/cdk-app.ts"
    ]
  },
  "keywords": [
    "rag",
    "document-ingestion",
    "serverless",
    "aws",
    "ondemandenv"
  ],
  "author": "OndemandEnv Platform",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/odmd-rag/rag-document-ingestion-service.git"
  }
}
